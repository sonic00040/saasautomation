# Supabase MCP Configuration
# Copy this file to .env and fill in your actual values

# Your Supabase Personal Access Token
# Get this from: Supabase Dashboard > Profile > Access Tokens
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here

# Your Supabase Project Reference ID
# Get this from: Supabase Dashboard > Settings > General > Project Reference
SUPABASE_PROJECT_REF=your_project_ref_here

# Backend API Configuration
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Security Configuration
TELEGRAM_WEBHOOK_SECRET=your_webhook_secret_here

# Application Configuration
LOG_LEVEL=INFO
ENVIRONMENT=development

# Redis Configuration (for rate limiting)
REDIS_URL=redis://localhost:6379

# Backend URL (for production, update to your deployed backend URL)
BACKEND_URL=http://localhost:8000
